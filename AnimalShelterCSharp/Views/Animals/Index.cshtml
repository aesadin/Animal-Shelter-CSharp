@{
  Layout = "_Layout";
}

@using AnimalShelter.Models;
@model List<AnimalShelter.Models.Animal>;

<h1>Animals</h1> 

<ul>
@foreach (Animal animal in Model)
{
  <li>@Html.ActionLink($"{animal.Type}","Details", new { id = animal.AnimalId})</li>
}
</ul>

<h3>@Html.ActionLink("Add new animal", "Create")</h3>
<h3>@Html.ActionLink("Go back home", "Index", "Home")</h3>